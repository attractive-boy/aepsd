<view class="example-search">
  <t-search placeholder="搜索" />
</view>
<t-tabs defaultValue="{{0}}"  bind:change="onTabsChange" bind:click="onTabsClick">
  <t-tab-panel  label="视频" value="0" >
    <t-grid class="block" column="{{2}}">
      <t-grid-item wx:for="{{ae}}" wx:key="index" text="{{item.title}}" image="{{staticUrl + item.video_cover}}" t-class-image="grid-image" bind:tap="toEdit" data-item="{{item}}"/>
    </t-grid>
  </t-tab-panel>
  <t-tab-panel  label="图片" value="1" >
    <t-grid class="block" column="{{2}}">
      <t-grid-item wx:for="{{psd}}" wx:key="index" text="{{item.title}}" image="{{staticUrl + item.video_cover}}" t-class-image="grid-image" bind:tap="toEdit" data-item="{{item}}"/>
    </t-grid>
  </t-tab-panel>
</t-tabs>
